[Atoms]                                                                                                                                                                                       
[[1]]                                                                                                                                                                                         
Hamiltonian     = Density                                                                                                                                                                     
Nd              = 1                                                                                                                                                                           
Udd             = 2.0 #-> Hubbard U                                                                                                                                                           
[[2]]                                                                                                                                                                                         
Hamiltonian     = Density                                                                                                                                                                     
Nd              = 1                                                                                                                                                                           
Udd             = 2.0 #-> Hubbard U                                                                                                                                                           
[[3]]                                                                                                                                                                                         
Hamiltonian     = Density                                                                                                                                                                     
Nd              = 1                                                                                                                                                                           
Udd             = 2.0 #-> Hubbard U                                                                                                                                                           
                                                                                                                                                                                              
[QMC]                                                                                                                                                                                         
Eigenbasis      = 1                                                                                                                                                                           
                                                                                                                                                                                              
Nwarmups        = 1e7                                                                                                                                                                         
Nmeas           = 1e6   
NCorr           = 20 #-> Wie viele Schritte zwischen zwei Messungen.                                                                                                                         
                                                                                                                                                                                              
NLookup_nfft    = 10000000                                                                                                                                                                     
                                                                                                                                                                                              
Ntau            = 2000           # # of points in tau  ->  Nur für Ausgabe wichtig!                                                                                                            
Niw             = 1000           # # of matsubara freq.                                                                                                                                        
